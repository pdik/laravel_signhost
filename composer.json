{
    "name": "pdik/laravel_signhost",
    "description": "A laravel package for Signhost",
     "homepage": "https://github.com/pdik/laravel_signhost",
    "license": "MIT",
    "authors": [
        {
            "name": "pepijn dik",
            "email": "pepijn@pdik.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
         "illuminate/pagination": "^8.31",
        "illuminate/routing": "^8.29",
        "illuminate/support": "^8.0",
        "illuminate/view": "^8.32"
    },
     "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
     "autoload": {
        "psr-4": {
            "Pdik\\laravel_signhost\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pdik\\laravel_signhost\\SignhostServiceProvider"
            ],
            "aliases": {
                "Signhost": "Pdik\\laravel_signhost\\SignhostFacade"
            }
        }
    }
}
