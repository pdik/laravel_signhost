{
  "name": "pdik/laravel_signhost",
  "description": "A laravel package for the Signhost api",
  "homepage": "https://github.com/pdik/laravel_signhost",
  "type": "package",
  "license": "mit",
  "keywords": [
    "pdik",
    "ondertekenen",
    "signhost",
    "client",
    "laravel",
    "api"
  ],
  "authors": [
    {
      "name": "pepijn dik",
      "email": "pepijn@pdik.nl"
    }
  ],
  "require": {
    "php": "^7.4",
    "illuminate/database": "^8.36",
    "illuminate/pagination": "^8.31",
    "illuminate/routing": "^8.29",
    "illuminate/support": "^8.0",
    "illuminate/view": "^8.32",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "pdik\\signhost\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "pdik\\signhost\\SignhostServiceProvider"
      ],
      "aliases": {
        "Signhost": "pdik\\signhost\\SignhostFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
